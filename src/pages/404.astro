---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Not Found">
  <div class="flex flex-col gap-10 justify-center items-center h-[60vh]">
    <img
      class="w-1/2 sm:w-1/3"
      src="/assets/404.svg"
      alt="404 Page not found.."
    />
    <p class="font-semibold text-xl">
      Please use navigation bar to navigate to desired page
    </p>
  </div>
</Layout>
